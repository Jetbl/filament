component Mac<G, L>(
  @within(G, G+1) a: 32,
  @within(G, G+1) b: 32,
  @within(G+2, G+3) c: 32,
  @exact(G, G+1) go: 1,
) -> (
  @within(G, G+L+1) out: 32,
  @exact(G, G+L+1) valid: 1
) -> {

  // Required cells for constructing this component.
  cells {
    M: Mult;
    A: Add;
  }

  control {
    m0 = M<G>(a, b);
    a0 = A<m0.done>(m0.out, c);
  }
}


